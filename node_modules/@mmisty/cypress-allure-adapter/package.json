{
  "name": "@mmisty/cypress-allure-adapter",
  "version": "0.15.2",
  "description": "cypress allure adapter to generate allure results during tests execution (Allure TestOps compatible)",
  "main": "index.js",
  "repository": "git@github.com:mmisty/cypress-allure-adapter.git",
  "homepage": "https://github.com/mmisty/cypress-allure-adapter#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "cypress",
    "e2e",
    "tests",
    "typescript",
    "allure",
    "TestOps",
    "plugins",
    "reporter",
    "cypress-allure-adapter",
    "@mmisty/cypress-allure-adapter"
  ],
  "peerDependencies": {
    "cypress": ">= 11.x"
  },
  "license": "MIT",
  "author": "Taisia Pitko (mmisty) <minydas@yandex.ru>",
  "overrides": {
    "loader-utils": "1.4.2"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": "./reports/jest",
    "outputName": "junit.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^19.1.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@cypress/code-coverage": "^3.12.9",
    "@ephesoft/webpack.istanbul.loader": "^2.2.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@mmisty/cypress-grep": "^1.9.2",
    "@types/cors": "^2.8.16",
    "@types/debug": "^4.1.11",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^18.11.9",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "allure-commandline": "^2.24.1",
    "cypress": "^13.4.0",
    "cypress-redirect-browser-log": "^1.2.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "istanbul-lib-coverage": "^3.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-test-each": "^0.9.1",
    "nyc": "^15.1.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^4.9.3",
    "webpack": "^5.89.0"
  },
  "dependencies": {
    "@mmisty/cypress-tags": "^1.0.14",
    "allure-js-commons": "^2.9.2",
    "allure-js-parser": "^0.0.7",
    "debug": "^4.3.4",
    "events": "^3.3.0",
    "fast-glob": "^3.3.2",
    "uuid": "^9.0.1",
    "uuid-by-string": "^4.0.0",
    "ws": "^8.14.2"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "integration/cucumber-steps/**/*.{js,ts}",
      "integration/cucumber/**/*.{js,ts}"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "volta": {
    "node": "18.12.0",
    "npm": "8.19.2"
  }
}
